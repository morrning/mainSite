{% block jdate_widget %}
        <div class="input-group mb-2">
            <div class="input-group-prepend">
                <div class="input-group-text"><i class="material-icons">list</i></div>
            </div>
            <input type="text" {{ block('widget_attributes') }} autocomplete="off" class="JdateInput form-control jdateInputTextbox{{ form.vars.id }}" onblur="SysCheckDateInput(this.value,'{{ form.vars.id }}');" placeholder="فرمت صحیح XXXX/XX/XX ">
            <div class="input-group-append">
                <span class="input-group-text"><i class="material-icons jdateIconValidation{{ form.vars.id }}">close</i></span>
            </div>
        </div>

{% endblock %}

